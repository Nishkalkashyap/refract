{
  "name": "refract",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.28.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --filter vite-example-app dev",
    "build": "pnpm -r --if-present build",
    "lint": "pnpm -r --if-present lint",
    "typecheck": "pnpm -r --if-present typecheck"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@eslint/js": "latest",
    "eslint": "latest",
    "globals": "latest",
    "turbo": "latest",
    "typescript": "latest",
    "typescript-eslint": "latest"
  }
}
