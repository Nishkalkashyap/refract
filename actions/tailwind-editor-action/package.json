{
  "name": "@nkstack/refract-tailwind-editor-action",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js"
    },
    "./server": {
      "types": "./dist/server-plugin.d.ts",
      "import": "./dist/server-plugin.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@babel/generator": "latest",
    "@babel/parser": "latest",
    "@babel/traverse": "latest",
    "@babel/types": "latest",
    "@nkstack/refract-tool-contracts": "workspace:*",
    "@nkstack/tailwind-editor-react": "^0.1.1",
    "react": "latest"
  },
  "devDependencies": {
    "@types/babel__generator": "latest",
    "@types/babel__traverse": "latest",
    "@types/react": "latest"
  },
  "scripts": {
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist",
    "build": "pnpm run clean && tsc -p tsconfig.build.json",
    "prepack": "pnpm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
